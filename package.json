{
  "name": "calender_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/app.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/app.css -w",
    "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --runInBand --coverage --watch",
    "test:c": "jest --coverage --watchAll=false",
    "test:u": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --runInBand --coverage --watch --updateSnapshot",
    "slate": "rimraf node_modules && rimraf package-lock.json && npm install"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.8.3",
    "enzyme-to-json": "3.4.3",
    "graphql-tag": "2.10.1",
    "handsontable": "10.0.0",
    "lodash": "4.17.15",
    "lodash.clonedeep": "4.5.0",
    "lodash.merge": "4.6.2",
    "moment": "^2.26.0",
    "react": "16.12.0",
    "react-apollo": "3.1.3",
    "react-app-polyfill": "0.1.3",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.0.1",
    "resolve": "1.8.1",
    "tailwindcss": "^1.4.6",
    "url-loader": "1.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@apollo/react-testing": "3.1.3",
    "@babel/cli": "7.8.3",
    "@babel/compat-data": "7.8.0",
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-transform-strict-mode": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "apollo-client": "2.6.4",
    "autoprefixer": "^9.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "0.2.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "cross-env": "6.0.3",
    "css-loader": "1.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint-config-airbnb": "17.1.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "extract-text-webpack-plugin": "3.0.2",
    "husky": "1.3.1",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "8.2.1",
    "postcss-cli": "^7.1.1",
    "react-test-renderer": "^16.13.1",
    "rosie": "2.0.1",
    "style-loader": "0.23.1",
    "waait": "^1.0.5"
  }
}
